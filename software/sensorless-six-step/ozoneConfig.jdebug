/*********************************************************************
*                                                                     
*       OnProjectLoad                                                 
*                                                                     
* Function description                                                
*   Project load routine. Required.                                   
*                                                                     
**********************************************************************
*/                                                                    
void OnProjectLoad (void) {
  //
  // Dialog-generated settings
  //
  Project.AddPathSubstitute ("D:/Johannes/Maker/ESC/Cube/Codegen", "$(ProjectDir)");
  Project.AddPathSubstitute ("d:/johannes/maker/esc/cube/codegen", "$(ProjectDir)");
  Project.SetDevice ("STM32G473CE");
  Project.SetHostIF ("USB", "");
  Project.SetTargetIF ("SWD");
  Project.SetTIFSpeed ("4 MHz");
  Project.AddSvdFile ("$(InstallDir)/Config/CPU/Cortex-M4F.svd");
  //
  // User settings
  //
  Edit.SysVar (VAR_HSS_SPEED, FREQ_10_KHZ);
  Edit.Preference (PREF_TIMESTAMP_FORMAT, TIMESTAMP_FORMAT_INST_CNT);
  File.Open ("$(ProjectDir)/build/Codegen.elf");
  Project.SetOSPlugin ("FreeRTOSPlugin");
}

